<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.WebViewActivity">

    <com.example.xuxiaopeng002.myapplication.view.CommonTitleView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:back_img="@drawable/ic_back"
        app:status_color="@color/color_blue"
        app:title_color="@color/color_blue"
        app:title_content_Text="@string/app_name" />

    <Button
        android:id="@+id/click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="点击在线加载PDF" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</LinearLayout>