<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.MyRecycleViewActivity">
    <com.example.xuxiaopeng002.myapplication.view.CommonTitleView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:status_color="@color/color_blue"
        app:title_color="@color/color_blue"
        app:back_img="@drawable/ic_back"
        app:title_content_Text="@string/app_name" />
    <com.example.xuxiaopeng002.myapplication.view.ReboundRecycleView
        android:id="@+id/m_recycler_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>