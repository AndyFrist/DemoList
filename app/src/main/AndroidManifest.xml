<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ex_xuxiaopeng002.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.MyRecycleViewActivity"></activity>
        <activity android:name=".activity.ShareElementActivity"
            android:theme="@style/Transparent"
            />

        <activity android:name=".activity.AnimatorActivity" />
        <activity android:name=".activity.WebViewActivity" />
        <activity android:name=".activity.SensorManagerActivity" />
        <activity android:name=".activity.HistogramActivity" />
        <activity android:name=".activity.MainActivitys">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>